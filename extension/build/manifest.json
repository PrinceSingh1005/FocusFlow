{
    "manifest_version": 3,
    "name": "FocusFlow",
    "version": "1.0",
    "description": "A productivity tracker to help you focus.",
    "permissions": [
        "storage",
        "tabs",
        "alarms",
        "idle",
        "declarativeNetRequest"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}